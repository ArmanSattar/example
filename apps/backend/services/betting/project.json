    "deploy": {
      "executor": "@berenddeboer/nx-sst:sst",
      "options": {
        "command": "deploy"
      }
    },
    "remove": {
      "executor": "@berenddeboer/nx-sst:sst",
      "options": {
        "command": "remove"
      }
