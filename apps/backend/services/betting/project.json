  "projectType": "application",
  "tags": [],
  "targets": {
    "dev": {
      "executor": "@berenddeboer/nx-sst:sst",
      "options": {
        "command": "dev"
      }
    },
    "diff": {
      "executor": "@berenddeboer/nx-sst:sst",
